{
    "executionRoleArn": "arn:aws:iam::account_ID:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "flask-bootstrap-staging-service",
            "image": "IMAGE1_NAME",
            "essential": true,
            "portMappings": [
                {
                    "protocol": "tcp",
                    "containerPort": 8000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "flask-bootstrap-staging"
}