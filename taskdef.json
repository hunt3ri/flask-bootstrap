{
    "family": "tutorial-task-def",
        "networkMode": "awsvpc",
        "containerDefinitions": [
            {
                "name": "flask-bootstrap-staging-service",
                "image": "<IMAGE1_NAME>",
                "portMappings": [
                    {
                        "containerPort": 8000,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,
            }
        ],
        "requiresCompatibilities": [
            "FARGATE"
        ],
        "cpu": "256",
        "memory": "512",
        "executionRoleArn": "arn:aws:iam::account_ID:role/ecsTaskExecutionRole"
}